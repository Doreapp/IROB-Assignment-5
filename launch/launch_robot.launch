<?xml version="1.0" encoding="UTF-8"?>
<launch>

<arg name="level" default="F"/>

  <node pkg="rosservice" type="rosservice" name="set_robot_pose" output="screen" args="call /gazebo/set_model_state '{model_state: { model_name: tiago_steel, pose: { position: { x: 0.0, y: -0.03, z: -0.001 }, orientation: {x: 0.0028936, y: -0.0003567, z: -0.4057067, w: 0.9139987 } }, twist: { linear: {x: 0 , y: 0, z: 0 } , angular: { x: 0, y: 0, z: 0 } } , reference_frame: map } }'" if="$(eval level == 'F')"/>

  <node pkg="rosservice" type="rosservice" name="set_robot_pose" output="screen" args="call /gazebo/set_model_state '{model_state: { model_name: tiago_steel, pose: { position: { x: -1.146, y: -6.155, z: -0.001 }, orientation: {x: 0, y: 0, z: -0.7149132, w: 0.6992132 } }, twist: { linear: {x: 0 , y: 0, z: 0 } , angular: { x: 0, y: 0, z: 0 } } , reference_frame: world } }'" if="$(eval level == 'E')"/>

  <node pkg="rosservice" type="rosservice" name="set_robot_pose" output="screen" args="call /gazebo/set_model_state '{model_state: { model_name: tiago_steel, pose: { position: { x: -1.146, y: -6.155, z: -0.001 }, orientation: {x: 0, y: 0, z: -0.7149132, w: 0.6992132 } }, twist: { linear: {x: 0 , y: 0, z: 0 } , angular: { x: 0, y: 0, z: 0 } } , reference_frame: world } }'" if="$(eval level == 'C')"/>

  <node pkg="rosservice" type="rosservice" name="set_robot_pose" output="screen" args="call /gazebo/set_model_state '{model_state: { model_name: tiago_steel, pose: { position: { x: 2.40, y: -11.0, z: -0.003 }, orientation: {x: 0, y: 0, z: 0.5735764, w: 0.819152 } }, twist: { linear: {x: 0 , y: 0, z: 0 } , angular: { x: 0, y: 0, z: 0 } } , reference_frame: map } }'" if="$(eval level == 'A')"/>

</launch>